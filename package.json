    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "csswring": "^6.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lost": "^8.2.0",
    "nib": "^1.1.2",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
