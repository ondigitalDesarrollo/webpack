{
  "name": "course-webpack",
  "version": "0.1.0",
  "description": "Curso de Webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-image/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./url-loader-videos/webpack.config.js",
    "build:json": "webpack --config ./url-json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js"
  },
  "keywords": [
    "JavaScript",
    "webpack"
  ],
  "author": "@restrepo_arnold",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "json-loader": "^0.5.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
